emonState.loading) {\n      return <p>Loading...</p>;\n    }\n\n    if (pokemonState.errorMsg !== \"\") {\n      return <p>{pokemonState.errorMsg}</p>;\n    }\n\n    return <p>unable to get data</p>;\n  };\n\n  return (\n    <div className={\"pokemon-container\"}>\n      <h1>{pokemonName}</h1>\n      {ShowData()}\n    </div>\n  );\n};\n\nexport default Pokemon;\n","/home/camillabachna/Desktop/Platzi/Platzi Courses/platzi-courses/react-router-redux/pokemon/src/reducers/PokemonListReducer.js",[],"/home/camillabachna/Desktop/Platzi/Platzi Courses/platzi-courses/react-router-redux/pokemon/src/actions/pokemonActions.js",[],"/home/camillabachna/Desktop/Platzi/Platzi Courses/platzi-courses/react-router-redux/pokemon/src/reducers/PokemonMultipleReducer.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":17,"column":6,"nodeType":"54","endLine":17,"endColumn":8,"suggestions":"55"},{"ruleId":"52","severity":1,"message":"56","line":14,"column":6,"nodeType":"54","endLine":14,"endColumn":8,"suggestions":"57"},"no-native-reassign",["58"],"no-negated-in-lhs",["59"],"react-hooks/exhaustive-deps","React Hook React.useEffect has a missing dependency: 'FetchData'. Either include it or remove the dependency array.","ArrayExpression",["60"],"React Hook useEffect has missing dependencies: 'dispatch' and 'pokemonName'. Either include them or remove the dependency array.",["61"],"no-global-assign","no-unsafe-negation",{"desc":"62","fix":"63"},{"desc":"64","fix":"65"},"Update the dependencies array to be: [FetchData]",{"range":"66","text":"67"},"Update the dependencies array to be: [dispatch, pokemonName]",{"range":"68","text":"69"},[515,517],"[FetchData]",[439,441],"[dispatch, pokemonName]"]